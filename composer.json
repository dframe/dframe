{
    "name": "dframe/dframe",
    "description": "PHP Framework - Dframe",
    "keywords": [
        "framework",
        "dframe",
        "mvc",
        "hmvc",
        "php"
    ],
    "homepage": "https://dframeframework.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/simple-cache": "^1.0"
    },
    "replace": {
        "dframe/config": "self.version",
        "dframe/cron": "self.version",
        "dframe/loader": "self.version",
        "dframe/router": "self.version",
        "dframe/session": "self.version",
        "dframe/token": "self.version",
        "dframe/view": "self.version"
    },
    "suggest": {
        "smarty/smarty": "^3.1",
        "kriswallsmith/assetic": "^1.4",
        "patchwork/jsqueeze": "^2.0",
        "ptachoire/cssembed": "^1.0",
        "natxet/cssmin": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dframe\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "smarty/smarty": "^3.1",
        "kriswallsmith/assetic": "^1.4",
        "patchwork/jsqueeze": "^2.0",
        "ptachoire/cssembed": "^1.0",
        "twig/twig": "^3.3",
        "mikey179/vfsstream": "^1.6",
        "natxet/cssmin": "^3.0"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    }
}
