        "php asset",
        "dframe"
    ],
    "homepage": "https://dframeframework.com/en/docs/dframe/master/asset/overview",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "require": {
